local function createBillboardGui(text, textColor, strokeColor)
    local billboardGui = Instance.new("BillboardGui")
    billboardGui.Size = UDim2.new(0, 100, 0, 50)
    billboardGui.StudsOffset = Vector3.new(0, 3, 0)
    billboardGui.Adornee = nil

    local textLabel = Instance.new("TextLabel", billboardGui)
    textLabel.Size = UDim2.new(1, 0, 1, 0)
    textLabel.BackgroundTransparency = 1
    textLabel.Text = text
    textLabel.TextColor3 = textColor
    textLabel.TextScaled = true
    textLabel.Font = Enum.Font.SciFi

    -- Set stroke (outline)
    textLabel.TextStrokeColor3 = strokeColor
    textLabel.TextStrokeTransparency = 0 -- Solid outline (0 is fully opaque)

    return billboardGui
end

local function addGuiToPlayer(player)
    local text, textColor, strokeColor

    if table.find(OWNER, player.Name) then
        text = "SV1 OWNER"
        textColor = Color3.new(0, 0, 0) -- Black text
        strokeColor = Color3.new(1, 1, 1) -- White stroke
    elseif table.find(DEVELOPER, player.Name) then
        text = "SV1 DEVELOPER"
        textColor = Color3.new(0, 0, 0) -- Black text
        strokeColor = Color3.new(0, 0, 0) -- No stroke
    elseif table.find(ADMIN, player.Name) then
        text = "SV1 ADMIN"
        textColor = Color3.new(1, 0, 0) -- Red text
        strokeColor = Color3.new(0, 0, 0) -- No stroke
    end

    print("Adding GUI for: " .. player.Name)
    print("Role: " .. (text or "No Role"))

    if text and textColor then
        player.CharacterAdded:Connect(function(character)
            local head = character:WaitForChild("Head")
            if player ~= game.Players.LocalPlayer then
                local billboardGui = createBillboardGui(text, textColor, strokeColor)
                billboardGui.Adornee = head
                billboardGui.Parent = head
                print("Billboard GUI added for: " .. player.Name)
            end
        end)

        if player.Character then
            local head = player.Character:FindFirstChild("Head")
            if head and player ~= game.Players.LocalPlayer then
                local billboardGui = createBillboardGui(text, textColor, strokeColor)
                billboardGui.Adornee = head
                billboardGui.Parent = head
                print("Billboard GUI added for: " .. player.Name)
            end
        end
    end
end

